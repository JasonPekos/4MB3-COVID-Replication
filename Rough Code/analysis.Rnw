\frac{dQ_{i}(t)}{dt} &=  q(I_{i}(t))-(\gamma + \delta + \mu)(Q_{i}(t))\\
\frac{d(R_{i}(t))}{dt} &=  \gamma(Q_{i}(t)) + \eta(v_{i}(t))(S_{i}(t)) - (\zeta + \mu)(R_{i}(t))\\
\frac{d(D_{i}(t))}{dt} &=  \delta(Q_{i}(t)\\
\end{align*}
\end{center}

